net: "models/ShermanNet/train_val.prototxt"
test_iter: 1000
test_interval: 100
# this shit should be congruent with batch size
base_lr: 0.00003
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 2000
display: 20
max_iter: 10000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "models/ShermanNet/ImgNet_Kaggle"
# uncomment the following to default to CPU mode solving
solver_mode: GPU
type: "Nesterov"
